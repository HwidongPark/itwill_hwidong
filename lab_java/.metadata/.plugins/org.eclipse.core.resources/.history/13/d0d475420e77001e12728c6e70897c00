import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;

public class Test {
    public static void main(String[] args) {

        
        
        List<String> list = new ArrayList<>(Arrays.asList("a"));
        list.add("새 노트");
        list.add("노트1");
        list.add("노트2");
        
        System.out.println(list);
        
        
        
        
    }
}
